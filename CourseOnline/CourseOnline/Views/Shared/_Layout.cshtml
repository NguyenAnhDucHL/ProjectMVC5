@using CourseOnline.Models
@model LoginViewModel
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="~/Assets/materialize/css/materialize.css">
    <link rel="stylesheet" href="~/Assets/materialize/css/materialize.min.css">
    <link rel="stylesheet" href="~/Assets/materialize/js/materialize.js">
    <link rel="stylesheet" href="~/Assets/materialize/js/materialize.min.js">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Materialize</title>
</head>

<body>


    <nav class="blue lighten-1">

        <div class="nav-wrapper container">
            <a id="logo-container" href="#" class="brand-logo"><i class="material-icons">school</i></a>
            <ul class="right hide-on-med-and-down row">
                <div class="row">
                    <div class="col-2">
                        <li class="col-sm-2"><a href="#">Hướng dẫn</a></li>
                        <li class="col-sm-2"><a href="#">Tài nguyên</a></li>
                        <li class="col-sm-2"><a href="#">Khóa học</a></li>
                        <li class="col-sm-2"><a href="#">Luyện thi</a></li>
                        <li class="col-sm-2">
                            <div class="input-field col-2">
                                <input placeholder=" Search" id="first_name" type="text" class="validate">
                            </div>

                        </li>
                        <li class="col-sm-2" style="margin-top:0px;">
                            @{
                                ViewBag.Title = "Log in";
                            }
                            <section id="socialLoginForm">
                                @Html.Partial("_ExternalLoginsListPartial", new ExternalLoginListViewModel { ReturnUrl = ViewBag.ReturnUrl })
                            </section>
                        </li>
                    </div>
                </div>


            </ul>
        </div>


    </nav>
    @RenderBody()
    <!--JavaScript at end of body for optimized loading-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</body>
</html>